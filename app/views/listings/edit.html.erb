<% @page_title = "Update Listing" %>
<% @page_desc = "This area is to update a listing" %>

<%= link_to '<< Back to Listings', listings_path(:section_id => @section.id), class: "btn btn-link" %>

<%= form_for(@listing, :url => listing_path(@listing, :section_id => @section.id), html: {class: 'form-horizontal'}) do |f| %>
    <%= render(:partial => 'form', :locals => {:f => f}) %> 
<% end %>