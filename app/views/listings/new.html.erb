<% @page_title = "Create Listings" %>
<% @page_desc = "This area is to create a listing" %>

<%= link_to '<< Back to Listings', listings_path(:section_id => @section.id), class: "btn btn-link" %>

<%= form_for(@listing, :url => listings_path(:section_id => @section.id), html: {class: 'form-horizontal'}) do |f| %>
    <%= render(:partial => 'form', :locals => {:f => f}) %>
<% end %>